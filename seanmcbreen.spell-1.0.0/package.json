{
	"name": "Spell",
	"displayName": "[DEPRECATED] Spelling and Grammar Checker",
	"description": "[DEPRECATED] Web service is no longer active - Try `Code Spellchecker`.  See README for details.",
	"icon": "images/spellIcon.svg",
	"version": "1.0.0",
	"publisher": "seanmcbreen",
	"galleryBanner": {
		"color": "#5c2d91",
		"theme": "dark"
	},
	"license": "SEE LICENSE IN LICENSE.txt",
	"bugs": {
		"url": "https://github.com/Microsoft/vscode-spell-check/issues",
		"email": "smcbreen@microsoft.com"
	},
	"homepage": "https://github.com/Microsoft/vscode-spell-check/blob/master/README.md",
	"repository": {
		"type": "git",
		"url": "https://github.com/Microsoft/vscode-spell-check.git"
	},
	"engines": {
		"vscode": "^1.6.0"
	},
	"activationEvents": [
		"*"
	],
	"contributes": {
		"commands": [
			{
				"command": "SpellProvider.changeLanguage",
				"title": "Change Language",
				"category": "Spell Checker"
			}
		],
		"keybindings": [
			{
				"command": "editor.action.quickFix",
				"key": "Alt+."
			}
		],
		"configuration": {
			"title": "Spell options",
			"properties": {
				"spell.StopAsking": {
					"type": "boolean",
					"default": false,
					"description": "Spell: Don't ask again for deprication warning"
				}
			}
		}
	},
	"main": "./out/extension",
	"scripts": {
		"vscode:prepublish": "tsc -p ./",
		"compile": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install"
	},
	"dependencies": {
		"teacher": "^0.1.0",
		"request": "2.9.152",
		"xml2js": "0.1.13",
		"open": "^0.0.5"
	},
	"devDependencies": {
		"typescript": "^2.0.3",
		"vscode": "^1.0.0",
		"mocha": "^2.3.3",
		"@types/node": "^6.0.40",
		"@types/mocha": "^2.2.32"
	},
	"__metadata": {
		"id": "4080647e-d654-4079-aad0-96ead5af8010",
		"publisherId": "a21445f8-fd83-46c9-b9e2-17c8cba9b89e",
		"publisherDisplayName": "Sean McBreen"
	}
}